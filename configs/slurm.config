process {
    withName:get_cache {
        executor='slurm'
        queue="${params.queue}"
        cpus=2
        memory='40GB'
        time='4h'
        container = "${params.image_folder}vep-110.1.sif"
        containerOptions = "${params.extra_mounts} -B ${params.project_folder}:/workdir -B ${params.vep_raw_data}:/workdir/filter -B ${params.tmp}:/workdir/tmp"
    }

    withName:vep {
        executor='slurm'
        queue="${params.queue}"
        cpus=4
        memory='40GB'
        time='4h'
        container = "${params.image_folder}vep-110.1.sif"
        containerOptions = "${params.extra_mounts} -B ${params.project_folder}:/workdir -B ${params.vep_raw_data}:/workdir/filter -B ${params.tmp}:/workdir/tmp"
    }
}